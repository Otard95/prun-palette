{
  "manifest_version": 2,
  "name": "PrUn Palette",
  "version": "0.0.1",

  "description": "Adds a command palette to Prosperous Universe",

  "icons": {
    "48": "icon48.png",
    "96": "icon96.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://apex.prosperousuniverse.com/*"],
      "js": ["index.js"],
      "css": ["index.css"]
    }
  ]
}
